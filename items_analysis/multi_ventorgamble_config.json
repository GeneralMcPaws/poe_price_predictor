{
    "data": {
        "filename": "C:/Users/Digi/Desktop/poe_price_predictor/record_creator/record_creator/csv/unique/ventorgamble_values.csv",
        "columns": [
            "Close",
            "Volume"
        ],
        "sequence_length": 6,
        "train_test_split": 0.5,
        "normalise": true
    },
    "training": {
        "epochs": 300,
        "batch_size": 32
    },
    "model": {
        "loss": "mse",
        "optimizer": "adam",
        "save_dir": "saved_models",
        "layers": [
            {
                "type": "lstm",
                "neurons": 200,
                "input_timesteps": 5,
                "input_dim": 2,
                "return_seq": true
            },
            {
                "type": "lstm",
                "neurons": 100,
                "return_seq": false
            },
            {
                "type": "dropout",
                "rate": 0.3
            },
            {
                "type": "dense",
                "neurons": 1,
                "activation": "linear"
            }
        ]
    }
}